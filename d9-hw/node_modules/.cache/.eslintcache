  const { data } = await response.json();\n        // Aggiungi i lavori al tuo store\n        // Assicurati che il reducer gestisca correttamente questa azione\n      } else {\n        alert(\"Error fetching results\");\n      }\n    } catch (error) {\n      console.log(error);\n    }\n  };\n\n  const handleAddToFavorites = (job) => {\n    // Aggiungi il lavoro ai preferiti utilizzando l'azione\n    addToFavorites(job);\n  };\n\n  return (\n    <Container>\n      <Row>\n        <Col xs={10} className=\"mx-auto my-3\">\n          <h1 className=\"display-1\">Remote Jobs Search</h1>\n        </Col>\n        <Col xs={10} className=\"mx-auto\">\n          <Form onSubmit={handleSubmit}>\n            <Form.Control type=\"search\" value={query} onChange={handleChange} placeholder=\"type and press Enter\" />\n          </Form>\n        </Col>\n        <Col xs={10} className=\"mx-auto mb-5\">\n          {jobs.map((jobData) => (\n            <div key={jobData._id}>\n              <Job data={jobData} />\n              <button onClick={() => handleAddToFavorites(jobData)}>Add to Favorites</button>\n            </div>\n          ))}\n        </Col>\n      </Row>\n    </Container>\n  );\n};\n\nconst mapStateToProps = (state) => ({\n  jobs: state.jobs,\n});\n\nexport default connect(mapStateToProps, { addToFavorites })(MainSearch);\n","/Users/egidio/Desktop/3_Settimana REDUX + TypeScript/S11:L2 - Reducers multipli & operazioni asincrone/3_Consegna S11:L2/U3-W3-D2/d9-hw/src/components/Job.jsx",[],[],"/Users/egidio/Desktop/3_Settimana REDUX + TypeScript/S11:L2 - Reducers multipli & operazioni asincrone/3_Consegna S11:L2/U3-W3-D2/d9-hw/src/components/Favorites.jsx",[],[],"/Users/egidio/Desktop/3_Settimana REDUX + TypeScript/S11:L2 - Reducers multipli & operazioni asincrone/3_Consegna S11:L2/U3-W3-D2/d9-hw/src/redux/reducers/index.js",[],[],"/Users/egidio/Desktop/3_Settimana REDUX + TypeScript/S11:L2 - Reducers multipli & operazioni asincrone/3_Consegna S11:L2/U3-W3-D2/d9-hw/src/redux/